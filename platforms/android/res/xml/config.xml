<?xml version='1.0' encoding='utf-8'?>
<widget id="de.teemachen.teeuhr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TeeUhr</name>
    <description>Die ultimative Teeuhr um jede Teesorte optimal zuzubereiten.</description>
    <author email="support@teemachen.de" href="http://www.teemachen.de">TeeMachen Team</author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-navigation img="*" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="data:*" />
    <icon density="ldpi" src="www/res/icon/mipmap-mdpi/ic_launcher.png" />
    <icon density="mdpi" src="www/res/icon/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="www/res/icon/mipmap-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="www/res/icon/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="www/res/icon/www/res/icon/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="xxxmipmap-hdpi/ic_launcher.png" />
    <splash density="land-ldpi" src="www/res/screen/android/drawable-land-ldpi-screen.png" />
    <splash density="land-mdpi" src="www/res/screen/android/drawable-land-mdpi-screen.png" />
    <splash density="land-hdpi" src="www/res/screen/android/drawable-land-hdpi-screen.png" />
    <splash density="land-xhdpi" src="www/res/screen/android/drawable-land-xhdpi-screen.png" />
    <splash density="land-xxhdpi" src="www/res/screen/android/drawable-land-xxhdpi-screen.png" />
    <splash density="land-xxxhdpi" src="www/res/screen/android/drawable-land-xxxhdpi-screen.png" />
    <splash density="port-ldpi" src="www/res/screen/android/drawable-port-ldpi-screen.png" />
    <splash density="port-mdpi" src="www/res/screen/android/drawable-port-mdpi-screen.png" />
    <splash density="port-hdpi" src="www/res/screen/android/drawable-port-hdpi-screen.png" />
    <splash density="port-xhdpi" src="www/res/screen/android/drawable-port-xhdpi-screen.png" />
    <splash density="port-xxhdpi" src="www/res/screen/android/drawable-port-xxhdpi-screen.png" />
    <splash density="port-xxxhdpi" src="www/res/screen/android/drawable-port-xxxhdpi-screen.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
